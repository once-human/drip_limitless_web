		$('.wo_settings_page .hiddddddddd').fadeIn(200);
	}
$(function() {
  $('form.setting-general-form').ajaxForm({
    url: Wo_Ajax_Requests_File() + '?f=download_info',
    beforeSend: function() {
      $('.wo_settings_page').find('.disable_btn').text("<?php echo $wo['lang']['please_wait']; ?>").attr('disabled','disabled');
    },
    success: function(data) {
      scrollToTop();
      if (data.status == 200) {
      	$('#download_steps_comp').slideUp(500);
		$('.ready_to_down_info').fadeIn(200);
		$('.ready_to_down_info p').html(data.message);
		$('.wo_settings_page .hiddddddddd').fadeOut(200);
      } 
      $('.wo_settings_page').find('.disable_btn').text("<?php echo $wo['lang']['generate_file']; ?>").removeAttr("disabled");
    }
  });
});
</script>